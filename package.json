{
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-functions": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "webpack": "^3.10.0",
    "webpack-plugin-hash-output": "^1.3.0",
    "yarn": "^1.3.2"
  },
  "scripts": {
    "build": "webpack --progress",
    "dll": "webpack --config ./webpack/dll/webpack.config.js && webpack",
    "format": "prettier --write \"./**/*.js\" --ignore-path ./.gitignore",
    "precommit": "lint-staged",
    "initDependencies":
      "npm install yarn --save-dev && npm run installDependencies",
    "installDependencies": "yarn install",
    "removeDependencies": "rm -rf ./node_modules"
  },
  "lint-staged": {
    "*.{js,json}": ["npm run format", "git add"]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "md5": "^2.2.1",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
