{
  "name": "webpack-multiple-pages",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dll && npm run dev",
    "dev": "webpack-dev-server --config ./build/config/dev/webpack.config.js",
    "server:dist": "node build/scripts/server/dist.js",
    "dll": "webpack --config ./build/config/__dll/webpack.config.js --progress",
    "dll:check":
      "webpack --config ./build/config/__dll__check/webpack.config.js --progress",
    "build":
      "cross-env BABEL_ENV=production webpack --config ./build/config/prod/webpack.config.js",
    "build:no-cache": "npm run clean:cache && npm run build",
    "build:progress": "npm run build -- --progress",
    "build:analyze": "npm run build:progress -- --env.ANALYZE",
    "build:check": "npm run build:progress -- --env.CHECK",
    "build:server": "npm run build:progress && npm run server:dist",
    "build:watch": "npm run build:progress -- --watch",
    "build:debug":
      "cross-env BABEL_ENV=production node-nightly --inspect ./node_modules/webpack/bin/webpack.js --config ./build/config/prod/webpack.config.js --progress",
    "build:i18n": "node build/scripts/language",
    "dep:init": "npm install yarn --save-dev && npm run dep:install",
    "dep:install": "yarn install && npm run clean:cache",
    "test":
      "mocha --require babel-core/register --require module-alias/register \"./@(src|tests)/**/*.test.@(js|jsx)\"",
    "test:target":
      "mocha --require babel-core/register --require module-alias/register ",
    "clean:cache": "node ./build/scripts/clean/cache.js",
    "clean:dep": "node ./build/scripts/clean/dep.js",
    "clean:out-of-date": "node ./build/scripts/clean/out-of-date.js",
    "git:reset": "git checkout .",
    "git:status": "git status",
    "format": "prettier --write \"./**/*.js\" \"./**/*.jsx\" \"./**/*.vue\"",
    "remote:dep:reset":
      "npm run git:reset && npm run clean:dep && dep:init && npm run git:reset",
    "fix:node-sass": "npm rebuild --force node-sass",
    "precommit": "lint-staged",
    "postmerge": "node ./build/scripts/git/hooks/post-merge/index.js",
    "prepush": "node ./build/scripts/git/hooks/pre-push.js"
  },
  "eslintIgnore": ["dist", "node_modules", "*.test.js", "build/utils/lib"],
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier --write", "git add"]
  },
  "dependencies": {
    "@cjy0208/fastclick": "^1.0.6",
    "antd-mobile": "^2.1.6",
    "axios": "^0.17.1",
    "babel-core": "6.26.0",
    "babel-polyfill": "^6.26.0",
    "dayjs": "^1.7.5",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.5.0",
    "invariant": "^2.2.4",
    "js-bridge-adapter": "^0.0.2",
    "lodash": "^4.17.5",
    "md5": "^2.2.1",
    "mint-ui": "^2.2.13",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.2",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "rc-form": "^2.4.5",
    "re-modulex": "^1.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.0.7",
    "react-router-cache-route": "^1.4.5",
    "react-router-dom": "^5.0.0",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "tiny-scroll-listener": "^1.0.0",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.5.13",
    "vue-i18n": "^8.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "autodll-webpack-plugin": "^0.3.9",
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-jsx-css-modules": "^0.0.9",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-functions": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-pipeline": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babylon": "6.13.0",
    "cache-loader": "^1.2.2",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.18",
    "compression": "^1.7.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.0-rc.2",
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "happypack": "^4.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-include-assets-plugin": "^1.0.3",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "jsdom": "^11.7.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.1",
    "lint-staged": "^6.1.0",
    "markdown-loader": "^2.0.2",
    "mocha": "^5.0.5",
    "module-alias": "^2.2.0",
    "node-nightly": "^1.7.3",
    "node-sass": "^7.0.0",
    "postcss-combine-duplicated-selectors": "^4.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.0",
    "postcss-modules-values": "^1.3.0",
    "postcss-px-to-viewport": "^1.1.0",
    "postcss-sass": "^0.3.0",
    "postcss-scss": "^1.0.4",
    "postcss-viewport-units": "^0.1.6",
    "postcss-write-svg": "^3.0.1",
    "prettier": "1.10.2",
    "react-hot-loader": "^4.8.4",
    "sass-loader": "^6.0.6",
    "speed-measure-webpack-plugin": "^1.2.2",
    "style-loader": "^0.20.3",
    "thread-loader": "^1.2.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^2.11.1",
    "webpack-parallel-uglify-plugin": "^1.0.2",
    "webpack-plugin-hash-output": "^1.3.0",
    "yargs": "^11.0.0",
    "yarn": "^1.12.3"
  },
  "_moduleAliases": {
    "@helpers": "src/vendor/helpers @"
  }
}
